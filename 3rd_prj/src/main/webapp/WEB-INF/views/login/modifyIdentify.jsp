//전화번호 체크
function mobile_check()
{
	var oPhone = $('#phone');
    var phoneNum = $('#phone').val();
    if(phoneNum.length <=0){
    	AlertAddClass( oPhone , 'error');
    	oPhone.siblings('span').hide();
        $('#phone').parent().siblings('p').html('휴대폰 번호를 입력해주세요. ');
        return false;
    }
    //x 표시 노출
    oPhone.siblings('span').show();

    var regexp = /^[0-9]*$/
	if( !regexp.test(phoneNum) ) {
		AlertAddClass( oPhone , 'error');
        $('#phone').parent().siblings('p').html('-없이 숫자로 입력해주세요.');
        return false;
	}
    if(phoneNum.length > 11 || phoneNum.length < 10){
    	AlertAddClass( oPhone , 'error');
        $('#phone').parent().siblings('p').html('휴대폰 번호를 정확히 입력하세요.');
        return false;
    }
    else if(phoneNum.length == 11)
    {
        $("[name=mobile1]").val(phoneNum.substring(0, 3));
        $("[name=mobile2]").val(phoneNum.substring(3, 7));
        $("[name=mobile3]").val(phoneNum.substring(7, 11));

    }
    else if(phoneNum.length == 10)
    {
        $("[name=mobile1]").val(phoneNum.substring(0, 3));
        $("[name=mobile2]").val(phoneNum.substring(3, 6));
        $("[name=mobile3]").val(phoneNum.substring(6, 10));
    }
    var comNumArray = ["010" ,"011", "016", "017","018", "019"];
    var telComNum = phoneNum.substring(0, 3);

    if(jQuery.inArray(telComNum, comNumArray) == -1){
        AlertAddClass( oPhone , 'error');
        $('#phone').parent().siblings('p').html('휴대폰 번호를 정확히 입력하세요.');
        return false;
    }
	AlertAddClass( oPhone , 'ok');
    return true;
}

//생년월일 체크
function birth_check()
{
	var year = $("#year");
	var month = $("#month");
	var day = $("#day");
	var yearVal = $("#year").val();
	var monthVal = $("#month").val();
	var dayVal = $("#day").val();
	var msgBox = $('#birthMsg');
	var regexp = /^[0-9]*$/;

	msgBox.css('display','');
	if(yearVal == "" && monthVal == "" && dayVal == ""){
		msgBox.html('생년월일을 입력해주세요. ');
		AlertAddClass(year,'error');
		return false;
	}
	if(yearVal == "" || yearVal.length != 4){
		msgBox.html('태어난 년도를 숫자 4자로 입력해주세요.');
		AlertAddClass(year,'error');
		return false;
	}
	else if(monthVal == ""){
		msgBox.html('태어난 월을 입력해주세요.');
		AlertAddClass(year,'error');
		return false;
	}
	else if(dayVal == ""){
		msgBox.html('태어난 날을 입력해주세요.');
		AlertAddClass(year,'error');
		return false;
	}
	else if( !regexp.test(dayVal) ) {
		msgBox.html('태어난 일을 숫자로 입력해주세요.');
		AlertAddClass(year,'error');
		return false;
	}
	else if(!regexp.test(yearVal)){
		msgBox.html('태어난 년도를 숫자 4자로 입력해주세요.');
		AlertAddClass(year,'error');
		return false;
	}
	else if(parseInt(dayVal) <= 0 || parseInt(dayVal) > 31){
		msgBox.html('생년월일을 정확히 입력해주세요.');
		AlertAddClass(year,'error');
		return false;
	}

	var today = new Date();
	var birth = new Date(yearVal, monthVal, dayVal);
	$("[name=age]").val(calcAge(yearVal, monthVal, dayVal)); //나이 계산

	if(today <= birth || parseInt(yearVal) < 1901){
		msgBox.html('생년월일을 정확히 입력해주세요.');
		AlertAddClass(year,'error');
		return false;
	}
	else if( $("[name=age]").val() < 14 ){
		AlertAddClass(year,'ok');
		// 14세미만 핸드폰 input, SMS, EMS 체크박스 숨김
        $('.phone, #agree_list').hide();
        msgBox.css('display','block');
		msgBox.html('만 14세 미만의 어린이는 법정대리인의 동의가 필요합니다.<br/>다음 단계에서 동의를 받아주세요.');
	}
	else{
		$('.phone, #agree_list').show();
	}
	AlertAddClass(year,'ok');
	return true;
}
