function resultAuth(data)
{
    var nRet        = data.result;
    var datastring  = data.datastring;
    var day = $('#day').val();
	var month = $('#month').val();

	if(day.length == 1){
		day = "0" + "" + day;
	}
	if(month.length == 1){
		month = "0" + "" + month;
	}
	var szBirth = $('#year').val() + month + day;

    if(nRet != 1) {
    	$(".ipinTab").parents('dl').addClass('error');
        $(".ipinTab").parents('dd').siblings('p').html('본인인증 오류 :: ' + nRet);
        return;
	}

    if($.trim(String($("[name=userName]").val()).toUpperCase()) != data.uname || szBirth != data.birth
    	||$('[name=gender]:checked').val() != data.sex) {
    	$(".ipinTab").parents('dl').addClass('error');
        $(".ipinTab").parents('dd').siblings('p').html("가입 시 입력한 정보와 본인확인의 정보가 일치하지 않습니다. 다시 본인확인을 해주세요.");
        return;
    }


    checkIdExist(datastring, data.route);

}
