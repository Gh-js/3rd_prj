                var userID = data.USER_INFO.uid;
                location.hash = "FULL_SUCCESS";
                // 외부에 제공하는 로그인 일 경우 nExternalType 의 값이 0 아닌 아닌 양수가 제공됨 1: 팟프리카
                $("#container").load("join.php", {szWork : "FULL_SUCCESS", nExternalType: nJsExternalType, szRedirectUrl: ''}, function(){
                    $("div.membership_complete strong").html(userID);
                });
            }
            else{
                alert(msg);
            }
        },
        error : function(jqXHR, textStatus, errorThrown){
            alert("네트워크 연결 상태를 확인해주세요. "+" -" + textStatus);
        }
    });
}
</script>
