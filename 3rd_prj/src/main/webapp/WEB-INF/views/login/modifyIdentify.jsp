function password_check2()
{
    var $oPassword1 = $("[name=password1]");
    var $oPassword2 = $("[name=password2]");

    var $msgbox1 = $("[name=password1]").parent().parent().find("p");
    var $msgbox2 = $("[name=password2]").parent().parent().find("p");

    var pwd1 = String($oPassword1.val());
    var pwd2 = String($oPassword2.val());

    var uid = $("[name=uid]").val();
    $('#PwDefaultMsg').remove(); //default msg 지움

    if(pwd1.length <= 0 ) {
    	$oPassword1.siblings('span').hide();
        $msgbox1.html('비밀번호를 입력해주세요.');
        AlertAddClass($oPassword1,'error');
        return false;
    }
    if(pwd1.length > 15 ) {
    	alert('비밀번호는 최대 15자까지 입력 가능합니다.');
		$oPassword1.val($oPassword1.val().substr(0,15));
		AlertAddClass($oPassword1,'error');
        return false;
    }
    $oPassword1.siblings('span').show();
    if( pwd1.length < 10 || pwd1.length > 15){
        $msgbox1.html('10~15자로 입력해주세요.');
        AlertAddClass($oPassword1,'error');
        return false;
    }
    if( pwd1.indexOf(" ") != -1){
        $msgbox1.html('비밀번호에 공백을 사용하실 수 없습니다.');
        AlertAddClass($oPassword1,'error');
        $oPassword2.parents('dl').removeClass();
        return false;
    }
    if(pwd1.indexOf("&") != -1 ){
        $msgbox1.html('특수문자 \'&\'는 입력하실 수 없습니다.');
        AlertAddClass($oPassword1,'error');
        $oPassword2.parents('dl').removeClass();
        return false;
    }

    if( pwd1.indexOf(uid) != -1 && uid.length > 0){
        $msgbox1.html('비밀번호에 아이디를 사용할 수 없습니다.');
        AlertAddClass($oPassword1,'error');
        return false;
    }

    var oCheckSameChar = new RegExp(/(\w)\1\1/);
    if( oCheckSameChar.test(pwd1) ){
        $msgbox1.html('연속으로 동일한 문자 3자 이상 사용할 수 없습니다.');
        AlertAddClass($oPassword1,'error');
        return false;
    }
    var chk = 0;
 	if(pwd1.search(/[0-9]/g) != -1 ) chk ++;
 	if(pwd1.search(/[a-z]/ig)  != -1 ) chk ++;
 	if(pwd1.search(/[!@#$%^&*()?_~]/g)  != -1  ) chk ++;
 	if(chk < 2){
  		$msgbox1.html('영문 (대소문자 구별), 숫자, 특수문자 중 2가지 이상 조합하여 입력해 주세요.');
        AlertAddClass($oPassword1,'error');
 		return false;
 	}

    AlertAddClass($oPassword1,'ok');
    $msgbox1.html(" ");

    return true;
}
