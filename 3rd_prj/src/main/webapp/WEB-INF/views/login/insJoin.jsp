<script type="text/javascript">	
window.onload = function() {
    // '전체 동의' 체크박스
    var totalAgree = document.getElementById('terms_agree');
    
    // 개별 동의 체크박스 목록
    var agreeChks = document.querySelectorAll('.agree_chk');
    
    // '전체 동의' 체크박스 클릭 이벤트 핸들러
    totalAgree.addEventListener('change', function() {
        agreeChks.forEach(function(chk) {
            chk.checked = totalAgree.checked;
        });
    });

    // 개별 체크박스 클릭 이벤트 핸들러
    agreeChks.forEach(function(chk) {
        chk.addEventListener('change', function() {
            // 모든 개별 체크박스가 선택되었는지 확인
            var allChecked = Array.from(agreeChks).every(function(chk) {
                return chk.checked;
            });
            
            // '전체 동의' 상태 업데이트
            totalAgree.checked = allChecked;
        });
    });
};

function previewImage() {
    var fileInput = document.getElementById('insProfile');
    var imagePreview = document.getElementById('imagePreview');

    if (fileInput.files && fileInput.files[0]) {
        var reader = new FileReader();

        reader.onload = function (e) {
            // 이미지를 미리보이는 요소에 표시
            imagePreview.src = e.target.result;
            imagePreview.style.display = 'block'; // 이미지 보이기
        };

        // 선택한 이미지 파일을 읽어옴
        reader.readAsDataURL(fileInput.files[0]);
    }
}

</script>
		
