<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.co.daitdayoung.login">


	<select id="insId" resultType="String" parameterType="kr.co.daitdayoung.login.vo.FindIdVO">
	select ins_Id
	from INSTRUCTOR
	where ins_name= #{name} and ins_birth = #{birth}
	</select>
	


</mapper>