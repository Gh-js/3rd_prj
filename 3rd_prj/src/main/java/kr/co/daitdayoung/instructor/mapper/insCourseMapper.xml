<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.co.daitdayoung.instructor">

	<select id="selectMyCourses" resultType="kr.co.daitdayoung.instructor.domain.CourseDomain" parameterType="String">
	select *
	from courses
	where ins_id=#{ insId } 
	</select>

	<select id="selectOneCourse" resultType="kr.co.daitdayoung.instructor.domain.CourseDomain" parameterType="kr.co.daitdayoung.instructor.vo.CourseVO">
	select *
	from courses
	where ins_id=#{ insId } and cou_code=#{ couCode} 
	</select>




</mapper>